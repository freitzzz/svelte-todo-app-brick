{
	"name": "svelte-todo-app-brick",
	"description": "todo app showcasing the features provided in aplicacao_web brick (https:&#x2F;&#x2F;github.com&#x2F;dart-pacotes&#x2F;.bricks)",
	"author": "freitzzz",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint . --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.2.1",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.32.2",
		"eslint-plugin-unused-imports": "^2.0.0",
		"postcss": "^8.4.21",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.9"
	},
	"type": "module",
	"dependencies": {
		"rippleui": "^1.9.2",
		"typesafe-i18n": "^5.24.2"
	},
	"keywords": [
		"typescript",
		"prettier",
		"eslint",
		"jest",
		"git",
		"starter",
		"svelte-todo-app-brick"
	]
}
